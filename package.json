{
  "name": "bakewatt",
  "version": "1.0.0",
  "description": "Educational math game through baking activities",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "docs:functions": "ts-node scripts/generateFunctionDocs.ts"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^21.0.0",
    "class-transformer": "^0.5.1",
    "fp-ts": "^2.16.1",
    "inversify": "^6.0.2",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "reflect-metadata": "^0.2.2",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "@types/three": "^0.158.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.55.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "ts-node": "^10.9.1",
    "ts-morph": "^18.0.0"
  }
}
